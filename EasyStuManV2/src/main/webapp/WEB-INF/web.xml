<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         id="WebApp_ID" version="2.5">
    <display-name>Archetype Created Web Application</display-name>

    <servlet>
        <servlet-name>StudentListServlet</servlet-name>
        <servlet-class>org.afeng.servlet.StudentListServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>StudentListServlet</servlet-name>
        <url-pattern>/stu_list</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>StudentSearchServlet</servlet-name>
        <servlet-class>org.afeng.servlet.SearchStudentServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>StudentSearchServlet</servlet-name>
        <url-pattern>/search_stu</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>EditStudentServlet</servlet-name>
        <servlet-class>org.afeng.servlet.EditStudentServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EditStudentServlet</servlet-name>
        <url-pattern>/edit</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>UpdateStudentServlet</servlet-name>
        <servlet-class>org.afeng.servlet.UpdateStudentServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UpdateStudentServlet</servlet-name>
        <url-pattern>/update</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>DeletStudentServlet</servlet-name>
        <servlet-class>org.afeng.servlet.DeleteStudentServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DeletStudentServlet</servlet-name>
        <url-pattern>/delete</url-pattern>
    </servlet-mapping>


</web-app>
